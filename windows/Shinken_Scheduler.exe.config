<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
  <appSettings>
    <add key="pythonlauncher" value="C:\Python27\python.exe"/>

    <add key="Scheduler_parameters" value="-c @@INSTALLDIR@@\etc\schedulerd-windows.ini"/>
    <add key="Scheduler_service" value="@@INSTALLDIR@@\bin\shinken-scheduler.py"/>
    <add key="Scheduler_SrvOutput" value="true"/>
    <add key="Scheduler_SrvOutputfolder" value="@@INSTALLDIR@@\svclogs"/>

    <add key="verboselevel" value="Error"/>
    <add key="Shinken_Scheduler" value="logconfig\shinken-schedulerlogconf.xml"/>

  </appSettings>
</configuration>